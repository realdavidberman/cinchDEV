            <fieldText>&lt;p&gt;&lt;b&gt;&lt;u&gt;Agency statistics:&lt;/u&gt;&lt;/b&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Total updates to &quot;Agency&quot; field: {!countAgencySet}&lt;/li&gt;&lt;li&gt;By CCMID: {!countEnrollmentByCCMIDAgency}&lt;/li&gt;&lt;li&gt;By Address: {!countEnrollmentByAddressAgency}&lt;/li&gt;&lt;/ul&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>AgentSummaryDetails_found</name>
            <fieldText>&lt;p&gt;&lt;b&gt;&lt;u&gt;Agent statistics:&lt;/u&gt;&lt;/b&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;# of Agents under Agency: {!countAgentSet}&lt;/li&gt;&lt;li&gt;# Enrollments with Default Agent under Agency: {!countEnrollmentDefaultAgentsUnderAgency}&lt;/li&gt;&lt;li&gt;# Enrollments updated with valid Agent: {!countAgentsUpdated}&lt;/li&gt;&lt;/ul&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>countAgentSet</leftValueReference>
                    <operator>GreaterThanOrEqualTo</operator>
                    <rightValue>
                        <numberValue>1.0</numberValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>countEnrollmentDefaultAgentsUnderAgency</leftValueReference>
                    <operator>GreaterThanOrEqualTo</operator>
                    <rightValue>
                        <numberValue>1.0</numberValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>AgentSummaryDetails_not_found</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;color: rgb(228, 5, 32);&quot;&gt;Either there were no Agents (Contacts) under the Agency, or no Enrollments tied to the Agency and the default (CCMID not found) Agent.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>or</conditionLogic>
                <conditions>
                    <leftValueReference>countAgentSet</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <numberValue>0.0</numberValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>countEnrollmentDefaultAgentsUnderAgency</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <numberValue>0.0</numberValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>true</showHeader>
