    <description>Based on updates to the Agency (CCMI, address details), look for possible unmatched Enrollments by the updated CCMID and/or address and connect them. Then look for Agents.</description>
    <formulas>
        <name>formulaDefaultAgencyID</name>
        <dataType>String</dataType>
        <expression>{!$Label.Account_Enrollment_CCMID_Not_Found}</expression>
    </formulas>
    <formulas>
        <name>formulaDefaultAgentID</name>
        <dataType>String</dataType>
        <expression>{!$Label.Contact_Enrollment_CCMID_Not_Found}</expression>
    </formulas>
    <formulas>
        <name>formulaFirst2OfAgencyName</name>
        <dataType>String</dataType>
        <expression>LEFT({!Agency.Name},2)</expression>
    </formulas>
    <formulas>
        <description>First 3 characters of ShippingStreet</description>
        <name>formulaFirst3OfAddressAgency</name>
        <dataType>String</dataType>
        <expression>LEFT({!Agency.ShippingStreet},3)</expression>
    </formulas>
    <formulas>
        <description>Based on Contact record</description>
        <name>formulaFirst3OfAddressAgent</name>
        <dataType>String</dataType>
        <expression>UPPER(LEFT({!Agents_from_Agency.Account_Billing_Street_Address__c},3))</expression>
    </formulas>
    <formulas>
        <name>formulaFirst3OfAddressEnrollment</name>
        <dataType>String</dataType>
        <expression>UPPER(LEFT({!Default_Agent_Enrollments.Agent_Address__c},3))</expression>
    </formulas>
    <formulas>
        <name>formulaFirst4OfZipcodeAgency</name>
        <dataType>String</dataType>
        <expression>LEFT({!Agency.ShippingPostalCode},4)</expression>
    </formulas>
    <formulas>
        <name>formulaFirst4OfZipcodeAgent</name>
        <dataType>String</dataType>
        <expression>LEFT({!Agents_from_Agency.Account_Billing_Zip__c},4)</expression>
    </formulas>
    <formulas>
        <description>Enrollment-based</description>
        <name>formulaFirst4OfZipcodeEnrollment</name>
        <dataType>String</dataType>
        <expression>LEFT({!Default_Agent_Enrollments.Agent_Zip_Code__c},4)</expression>
    </formulas>
    <formulas>
        <description>Contact</description>
        <name>formulaFirstLetterAgentFirstName</name>
        <dataType>String</dataType>
        <expression>UPPER(LEFT({!Agents_from_Agency.FirstName},1))</expression>
    </formulas>
    <formulas>
        <name>formulaFirstLetterAgentFirstNameEnrollment</name>
        <dataType>String</dataType>
        <expression>UPPER(LEFT({!Default_Agent_Enrollments.Agent_First_Name__c},1))</expression>
    </formulas>
    <formulas>
        <name>formulaLastNameAgent</name>
        <dataType>String</dataType>
        <expression>UPPER({!Agents_from_Agency.LastName})</expression>
    </formulas>
    <formulas>
        <description>Agent&apos;s last name from Enrollment</description>
        <name>formulaLastNameEnrollment</name>
        <dataType>String</dataType>
        <expression>{!Default_Agent_Enrollments.Agent_Last_Name__c}</expression>
    </formulas>
